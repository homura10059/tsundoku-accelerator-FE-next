{
  "name": "tsundoku-accelerator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down -v",
    "dev:db:update": "prisma migrate dev --preview-feature && npx prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "prisma migrate deploy --preview-feature && next start -p $PORT"
  },
  "engines": {
    "node": "14.15.3"
  },
  "dependencies": {
    "@prisma/client": "2",
    "final-form": "^4.20.1",
    "next": "^10.0.4",
    "puppeteer": "^5.5.0",
    "puppeteer-autoscroll-down": "^0.1.6",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.2",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@prisma/cli": "2",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/puppeteer": "^5.4.2",
    "@types/ramda": "^0.27.34",
    "@types/react": "^17.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
