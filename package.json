{
  "name": "tsundoku-accelerator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down -v",
    "dev:db:update": "prisma migrate dev --preview-feature",
    "dev:db:generate": "npx prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "prisma migrate deploy --preview-feature && next start -p $PORT",
    "command:update:wishLists": "node schedulers/wishLists/update.js",
    "command:update:items": "node schedulers/items/update.js",
    "command:update:notifications": "node schedulers/notifications/update.js",
    "command:wakeup": "node schedulers/wakeUp.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "engines": {
    "node": "14.15.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.0.0",
    "@prisma/client": "^2.1.4",
    "@tailwindcss/postcss7-compat": "^2.1.0",
    "autoprefixer": "^9",
    "axios": "^0.21.1",
    "date-fns": "^2.16.1",
    "final-form": "^4.20.1",
    "next": "^10.0.4",
    "next-auth": "^3.1.0",
    "postcss": "^7",
    "puppeteer": "^5.5.0",
    "puppeteer-autoscroll-down": "^0.1.6",
    "ramda": "^0.27.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-final-form": "^6.5.2",
    "react-hook-form": "^7.0.0",
    "react-markdown": "^5.0.3",
    "styled-components": "^5.2.1",
    "styled-reset": "^4.3.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "zod": "^1.11.11"
  },
  "devDependencies": {
    "@prisma/cli": "2",
    "@storybook/addon-a11y": "^6.1.17",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-docs": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-knobs": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/addon-storyshots": "^6.1.17",
    "@storybook/addons": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@types/jest": "^26.0.19",
    "@types/next-auth": "^3.7.1",
    "@types/node": "^14.14.16",
    "@types/puppeteer": "^5.4.2",
    "@types/ramda": "^0.27.34",
    "@types/react": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "storybook-addon-next-router": "^2.0.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
